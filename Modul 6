data_hanif = read.csv("C:/Dataset/DirectMarketing.csv")
hist(data_hanif$AmountSpent)
hist(data_hanif$AmountSpent[data_hanif$Married=="Married"])
hist(data_hanif$AmountSpent[data_hanif$Married=="Single"])
d=density(data_hanif$AmountSpent)
plot(d)
d=density(data_hanif$AmountSpent[data_hanif$Married=="Married"])
plot (d)
